}

void mousePressed(){
  if(GameState == 0){
    GameState = 1;
  }
}

void keyPressed(){
  if(GameState == 1){
   if(key == 'q' || key == 'Q'){
     
     PokemonSelected=1;
     GameState = 2;
     
   }else if(key == 'w' || key == 'W'){
   PokemonSelected=2;
     GameState = 2;
   }else if(key == 'e' || key == 'E'){
   PokemonSelected=3;
     GameState = 2;
   }else if(key == 'r' || key == 'R'){
   PokemonSelected=4;
     GameState = 2;
   }else if(key == 't' || key == 'T'){
   PokemonSelected=5;
     GameState = 2;
   }
  }else if(GameState == 2){
    if(key == 's' || key == 'S'){
      GameState=3;
   }
  }else if(GameState == 3){
    if(key == 's' || key == 'S'){
      GameState=4;
   }
  }
  else if(GameState == 4){
    if(key == 'x' || key == 'X'){
      GameState=1;
   }
 }
}
